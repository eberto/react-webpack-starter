{
  "name": "react-webpack",
  "author": "Eberto Cepero Abreu",
  "license": "ISC",
  "version": "1.0.0",
  "description": "React webpack boilerplate",
  "private": true,
  "main": "webpack.config.js",
  "scripts": {
    "start": "./node_modules/.bin/npm-run-all --parallel start-client start-server",
    "start-prod": "node server.prod.js",
    "start-client": "node server.js",
    "start-server": "./node_modules/.bin/json-server db.json",
    "build": "./node_modules/.bin/npm-run-all webpack-build-vendor webpack-build",
    "build-prod": "./node_modules/.bin/npm-run-all webpack-build-vendor webpack-build-prod",
    "webpack-build": "./node_modules/.bin/webpack --display-chunks",
    "webpack-build-prod": "./node_modules/.bin/webpack --display-chunks --config webpack.config.prod.js",
    "webpack-build-vendor": "./node_modules/.bin/webpack --display-chunks --config webpack.config.vendor.js",
    "test": "./node_modules/.bin/karma start",
    "postinstall": "./node_modules/.bin/typings install"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^0.2.0",
    "autoprefixer": "^6.3.7",
    "awesome-typescript-loader": "^1.1.1",
    "bluebird": "^3.4.1",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.14.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-bluebird": "^1.0.3",
    "fetch-mock": "^5.0.5",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "img-loader": "^1.2.2",
    "jasmine-core": "^2.4.1",
    "jquery": "^3.0.0",
    "json-server": "^0.8.17",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.14.1",
    "node-sass": "^3.8.0",
    "open": "0.0.5",
    "path": "^0.12.7",
    "postcss-loader": "^0.9.1",
    "promise-defer": "^1.0.0",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "url-loader": "^0.5.7",
    "viperx": "^1.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-manifest-plugin": "^1.0.1",
    "webpack-md5-hash": "0.0.5"
  }
}
